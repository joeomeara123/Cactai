    "@types/react-dom": "^18.3.7",
    "dotenv": "^17.2.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.15",
    "husky": "^8.0.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
